{
  "name": "webpack-static-html-pages",
  "version": "1.0.0",
  "description": "Example of a static website assembled by using webpack v4",
  "keywords": [
    "webpack",
    "static",
    "starter",
    "html",
    "pages"
  ],
  "license": "MIT",
  "dependencies": {
    "beautify-html-webpack-plugin": "^1.0.3",
    "colorguard": "^1.2.1",
    "cq-prolyfill": "^0.4.0",
    "ejs": "^3.1.5",
    "extract-loader": "^5.1.0",
    "html-beautify-webpack-plugin": "^1.0.5",
    "html-webpack-link-type-plugin": "^1.0.4",
    "normalize.css": "^8.0.1",
    "postcss-inline-svg": "^5.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "stylefmt": "^6.0.3",
    "stylelint": "^13.8.0"
  },
  "scripts": {
    "start": "webpack serve --config webpack.dev.js --mode development",
    "start:dist": "http-server dist --port 15000",
    "build": "webpack --config webpack.prod.js --mode production",
    "preview": "npm run build && npm run start:dist",
    "test": "standard",
    "cy:run": "cypress run"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "cypress": "^6.1.0",
    "file-loader": "^6.2.0",
    "html-inline-css-webpack-plugin": "^1.9.1",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "http-server": "^0.12.3",
    "mini-css-extract-plugin": "^1.3.3",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "standard": "^16.0.3",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "standard": {
    "ignore": [
      "cypress"
    ],
    "env": [
      "node",
      "es6"
    ]
